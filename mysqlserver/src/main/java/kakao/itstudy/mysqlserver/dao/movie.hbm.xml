<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="kakao.itstudy.mysqlserver.domain">
	<class name="Movie" table="movie">
		<id name="movieid" column="movieid">
		</id>
		<property name="title" column="title" />
		<property name="subtitle" column="subtitle" />
		<property name="pubdate" column="pubdate" />
		<property name="director" column="director" />
		<property name="actor" column="actor" />
		<property name="genre" column="genre" />
		<property name="rating" column="rating" />
		<property name="thumbnail" column="thumbnail" />
		<property name="link" column="link" />
	</class>
</hibernate-mapping>